        url('/assets/icomoon/icomoon.ttf?jl3qi9') format('truetype'),
        url('/assets/icomoon/icomoon.woff?jl3qi9') format('woff'),
        url('/assets/icomoon/icomoon.svg?jl3qi9#icomoon') format('svg');
